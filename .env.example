# QORE Backend API - Environment Variables Example
# Copy this file to .env and fill in the appropriate values

# Server Configuration
PORT=3000
NODE_ENV=development

# Database Configuration
DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASSWORD=password
DB_NAME=qore_backend_db

# JWT Configuration
JWT_SECRET=your_jwt_secret_key
JWT_EXPIRES_IN=1d

# UltraVox API Configuration
ULTRAVOX_API_URL=https://api.ultravox.ai
ULTRAVOX_API_KEY=your_ultravox_api_key
ULTRAVOX_API_TIMEOUT=5000
ULTRAVOX_API_RETRIES=3

# QORE AI Integration Settings
QORE_INTERNAL_API_KEY=your_secure_qore_api_key
QOREAI_ENABLE_CACHING=true
QOREAI_CACHE_EXPIRATION=300
QOREAI_LOG_API_CALLS=true
QOREAI_MAX_REQUESTS_PER_MINUTE=60
